{
  "compilerOptions": {
    "strictPropertyInitialization": false,
    "target": "es5",
    "lib": ["dom", "dom.iterable", "ES2015"],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "sourceMap": true,
    "noEmit": true,
    "jsx": "react"
  },
  "include": ["lib"],
  "browserslist": ["> 5%"]
}
